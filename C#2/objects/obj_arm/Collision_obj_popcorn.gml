if(!(global.cupS + global.decrease = 5))
{
	if(mouse_check_button(mb_left))
	{
		if(other.sprite_index = spr_pop or
		   other.sprite_index = spr_pop2)
		{
			global.cupS += 1;
		}
		else if(other.sprite_index = spr_popTanuki or
				other.sprite_index = spr_popTanuki_Mimi or
				other.sprite_index = spr_popTanuki_Pop or
				other.sprite_index = spr_tanutanu)
		{
			global.decrease += 1;
		}
	}
}